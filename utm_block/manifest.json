{
  "name":"UTM block",
  "version":"1.0.0",
  "description":"A simple extension that removes UTM tags from the URL",
  "manifest_version":2,

  "icons":{
    "128":"icon_128.png",
    "48":"icon_48.png",
    "16":"icon_16.png"
  },
  "author":"Karl-Gustav Kallasmaa",
  "homepage_url":"https://github.com/gowizz/UTMBlock",
  "background":{
     "scripts": ["background.js"]
  },
  "permissions":[
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
